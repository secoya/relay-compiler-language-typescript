{
  "name": "@secoya/typescript-transform-relay",
  "version": "2.0.0",
  "description": "A transformer for use with TypeScript when using relay (modern/compat/classic)",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc -p .",
    "clean": "rm -Rf ./lib",
    "prepublishOnly": "npm run clean && npm run test && npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/secoya/relay-compiler-language-typescript.git"
  },
  "keywords": [
    "relay",
    "typescript"
  ],
  "author": "Kaare Hoff Skovgaard <kaare@kaareskovgaard.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/secoya/relay-compiler-language-typescript/issues"
  },
  "homepage": "https://github.com/secoya/relay-compiler-language-typescript#readme",
  "dependencies": {
    "graphql": "^14.0.2"
  },
  "devDependencies": {
    "@types/graphql": "^14.0.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.8",
    "jest": "^26.5.3",
    "relay-test-utils": "^1.7.0",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "typescript": "4.x"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost",
    "testRegex": "test/.+?-test\\.tsx?$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ]
  }
}
